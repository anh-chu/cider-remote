# Server Configuration
PORT=3001
NODE_ENV=production

# CORS Configuration
# Note: CORS_ORIGIN and CORS_METHODS are placeholders for future use
# Currently, the server accepts all origins (*)
CORS_ORIGIN=*
CORS_METHODS=GET,POST

# Logging
LOG_LEVEL=info

# Node.js Performance
NODE_OPTIONS=--max-old-space-size=512
